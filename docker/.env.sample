# Build-time environment variables for Docker
# These are dummy values used only during the build phase
# Real values should be provided at runtime via docker-compose env_file

BETTER_AUTH_SECRET=dummy-build-secret-replace-at-runtime
DATA_DIR=/tmp/build-data

# Optional configs with safe defaults
LOG_LEVEL=info
NO_COLOR=false
DISABLE_SIGNUPS=false
DISABLE_PASSWORD_AUTH=false
DB_WAL_MODE=true
